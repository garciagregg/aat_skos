<rdf:Description rdf:about="http://vocab.getty.edu/aat/300257514">
	<skos:prefLabel xml:lang="en">gauging</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">rimpelsteken</skos:prefLabel>
