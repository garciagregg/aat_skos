	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">guò dù qīng xǐ</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">guo du qing xi</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">kuo tu ch'ing hsi</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">te grondig reinigen</skos:prefLabel>
	<skos:prefLabel xml:lang="es">barrido</skos:prefLabel>
