<rdf:Description rdf:about="http://vocab.getty.edu/aat/300228803">
	<skos:prefLabel xml:lang="en">creasing (process)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">折皺</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">zhé zhòu</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">zhe zhou</skos:prefLabel>
