<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053525">
	<skos:prefLabel xml:lang="en">gelatin dry plate process</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">明膠乾版印相法</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">míng jiāo gān bǎn yìn xiàng fǎ</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">ming jiao gan ban yin xiang fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">ming chiao kan pan yin hsiang fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">droge gelatineplaatprocedés</skos:prefLabel>
	<skos:prefLabel xml:lang="es">proceso de gelatina sobre placa seca</skos:prefLabel>
	<skos:altLabel xml:lang="en">dry plate, gelatin</skos:altLabel>
	<skos:altLabel xml:lang="en">dry plate process (gelatin silver)</skos:altLabel>
	<skos:altLabel xml:lang="en">dry plate, silver gelatin</skos:altLabel>
	<skos:altLabel xml:lang="en">gelatin dry-plate</skos:altLabel>
	<skos:altLabel xml:lang="en">gelatin dry plate</skos:altLabel>
	<skos:altLabel xml:lang="en">gelatin dry-plate negative process</skos:altLabel>
	<skos:altLabel xml:lang="en">negative process, gelatin dry-plate</skos:altLabel>
	<skos:altLabel xml:lang="en">silver gelatin dry plate</skos:altLabel>
	<skos:altLabel xml:lang="zh-hant">明膠乾版印相過程</skos:altLabel>
	<skos:altLabel xml:lang="nl">droge gelatineplaatprocedé</skos:altLabel>
	<skos:altLabel xml:lang="es">procesado viscoso</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A photographic process employing silver in a gelatin binder on a glass photographic plate.</skos:scopeNote>
	<skos:scopeNote xml:lang="es">Proceso fotográfico que emplea plata en un fijador de gelatina sobre una placa fotográfica de vidrio.</skos:scopeNote>
	<skos:scopeNote xml:lang="nl">Ontwikkelgelatinezilverdruk waarbij gebruik wordt gemaakt van zilver in een gelatine-bindmiddel op een glazen fotografische plaat.</skos:scopeNote>
	<skos:scopeNote xml:lang="zh-hant">一種將明膠黏接劑中的銀，用於玻璃照片版上的攝影印相法。</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300139114"/>
	<skos:hasTopConcept rdf:resource="http://vocab.getty.edu/aat/300264090"/>
	<dc:identifier>300053525</dc:identifier>
