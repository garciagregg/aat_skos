	<skos:altLabel xml:lang="es">decolorar</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Any change in the color of an object.</skos:scopeNote>
	<skos:scopeNote xml:lang="es">Cualquier cambio en el color de un objeto.</skos:scopeNote>
