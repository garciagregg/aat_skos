</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300261977">
	<skos:prefLabel xml:lang="en">cornrowing</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">玉米髮式</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">yù mǐ fǎ shì</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">yu mi fa shi</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">yü mi fa shih</skos:prefLabel>
