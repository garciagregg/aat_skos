	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-01-03T11:17:07</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300391218">
	<skos:prefLabel xml:lang="en">proton probe microanalysis</skos:prefLabel>
	<skos:altLabel xml:lang="en">microanalysis, proton probe</skos:altLabel>
	<skos:altLabel xml:lang="en">proton probe x-ray microanalysis-</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Technique in microanalytical chemistry in which a finely focused proton beam is used to excite an x-ray spectrum.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300224706"/>
	<skos:hasTopConcept rdf:resource="http://vocab.getty.edu/aat/300264090"/>
