	<skos:prefLabel xml:lang="nl">dompelsolderen</skos:prefLabel>
	<skos:prefLabel xml:lang="es">soldadura por inmersión</skos:prefLabel>
	<skos:altLabel xml:lang="en">brazing, dip</skos:altLabel>
