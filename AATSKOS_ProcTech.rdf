<rdf:Description rdf:about="http://vocab.getty.edu/aat/300164032">
	<skos:prefLabel xml:lang="en-us">stovewood construction</skos:prefLabel>
	<skos:prefLabel xml:lang="en-gb">stove-wood construction</skos:prefLabel>
