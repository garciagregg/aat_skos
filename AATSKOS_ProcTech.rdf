<rdf:Description rdf:about="http://vocab.getty.edu/aat/300195856">
	<skos:prefLabel xml:lang="en">justification (typography)</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">uitlijnen</skos:prefLabel>
