<rdf:Description rdf:about="http://vocab.getty.edu/aat/300390002">
	<skos:prefLabel xml:lang="en">wetting-drying cycle</skos:prefLabel>
	<skos:altLabel xml:lang="en">cycle, wetting-drying</skos:altLabel>
	<skos:altLabel xml:lang="en">wetting-drying</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Refers to a natural or manmade soil moisture regime and its effects on the physical, chemical, and biological properties of irrigated soils.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300229438"/>
	<skos:hasTopConcept rdf:resource="http://vocab.getty.edu/aat/300264090"/>
