</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053757">
	<skos:prefLabel xml:lang="en">dispersion (process)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">分散 (過程)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">fēn sàn</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">fen san</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">dispergeren</skos:prefLabel>
	<skos:prefLabel xml:lang="es">disperción (proceso)</skos:prefLabel>
	<skos:altLabel xml:lang="en">dispersed</skos:altLabel>
	<skos:altLabel xml:lang="zh-hant">散佈</skos:altLabel>
	<skos:altLabel xml:lang="zh-latn-wadegile">fen san</skos:altLabel>
	<skos:altLabel xml:lang="nl">gedispergeerd</skos:altLabel>
	<skos:altLabel xml:lang="es">dispersión</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The action of diffusing or spreading, as in particulate matter in liquid suspension.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300053748"/>
