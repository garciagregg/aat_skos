	<skos:prefLabel xml:lang="zh-hant">分光光度學</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">fen guang guang du xue</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">fen kuang kuang tu hsüeh</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">spectrofotometrie</skos:prefLabel>
