<rdf:Description rdf:about="http://vocab.getty.edu/aat/300262450">
	<skos:prefLabel xml:lang="en">body painting</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">bodypainting</skos:prefLabel>
	<skos:prefLabel xml:lang="es">pintura corporal</skos:prefLabel>
