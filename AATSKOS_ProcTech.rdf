<rdf:Description rdf:about="http://vocab.getty.edu/aat/300263372">
	<skos:prefLabel xml:lang="en">acrophony</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">頂音法</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">dǐng yīn fǎ</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">ding yin fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">ting yin fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">acrofonie</skos:prefLabel>
	<skos:prefLabel xml:lang="es">acrofonía</skos:prefLabel>
