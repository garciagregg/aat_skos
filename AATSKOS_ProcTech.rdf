	<skos:prefLabel xml:lang="zh-hant">陰蒂切除術</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">yīn dì qiē chú shù</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">yin di qie chu shu</skos:prefLabel>
