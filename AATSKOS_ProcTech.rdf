<rdf:Description rdf:about="http://vocab.getty.edu/aat/300390600">
	<skos:prefLabel xml:lang="en">impact-echo</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Impact-echo is a nondestructive evaluation technique for masonry construction which detects flaws by monitoring impact-generated sound waves that are disseminated through a structure and are reflected by internal flaws and external surfaces.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300081683"/>
