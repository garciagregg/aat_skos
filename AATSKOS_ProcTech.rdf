<rdf:Description rdf:about="http://vocab.getty.edu/aat/300214718">
	<skos:prefLabel xml:lang="en">calorimetry</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">熱量測定法</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">rè liàng cè dìng fǎ</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">re liang ce ding fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">je liang ts'e ting fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">calorimetrie</skos:prefLabel>
	<skos:prefLabel xml:lang="es">calorimetría</skos:prefLabel>
	<skos:altLabel xml:lang="en">heat measurement</skos:altLabel>
	<skos:altLabel xml:lang="nl">warmtemeting</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The technique accomplishing the measurement of quantities of heat.</skos:scopeNote>
	<skos:scopeNote xml:lang="nl">De techniek voor het meten van hoeveelheden warmte.</skos:scopeNote>
	<skos:scopeNote xml:lang="es">Medición de la cantidad de calor.</skos:scopeNote>
	<skos:scopeNote xml:lang="zh-hant">完成熱量測量的技術。</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300053578"/>
