	<skos:prefLabel xml:lang="nl">aquarelleren</skos:prefLabel>
	<skos:prefLabel xml:lang="es">acuarela</skos:prefLabel>
	<skos:altLabel xml:lang="en-us">transparent watercolor</skos:altLabel>
	<skos:altLabel xml:lang="en-us">watercolor, transparent</skos:altLabel>
	<skos:altLabel xml:lang="en-gb">transparent watercolour</skos:altLabel>
	<skos:altLabel xml:lang="en-gb">water-colour, transparent</skos:altLabel>
	<skos:altLabel xml:lang="en-gb">watercolour, transparent</skos:altLabel>
