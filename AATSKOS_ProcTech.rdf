	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">yīng shì qì fǎ</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">ying shi qi fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">ying shih ch'i fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">staand verband</skos:prefLabel>
	<skos:prefLabel xml:lang="es">aparejo inglés</skos:prefLabel>
