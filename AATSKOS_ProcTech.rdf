	<skos:altLabel xml:lang="en-us">trichromatic processes</skos:altLabel>
	<skos:altLabel xml:lang="en-us">trichrome processes</skos:altLabel>
	<skos:altLabel xml:lang="en-us">tri-color processes</skos:altLabel>
	<skos:altLabel xml:lang="en-us">tricolor processes</skos:altLabel>
	<skos:altLabel xml:lang="en-gb">three-colour process (photography)</skos:altLabel>
