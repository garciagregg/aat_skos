	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2017-08-25T10:50:20</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053146">
	<skos:prefLabel xml:lang="en">swaging</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">型鍛</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">xíng duàn</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">xing duan</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">hsing tuan</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">smeden in zadels</skos:prefLabel>
