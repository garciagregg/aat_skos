	<skos:prefLabel xml:lang="nl">rollen (aardewerktechniek)</skos:prefLabel>
	<skos:prefLabel xml:lang="es">enrollado</skos:prefLabel>
	<skos:altLabel xml:lang="en">coiled (pottery technique)</skos:altLabel>
	<skos:altLabel xml:lang="en">coil building (pottery technique)</skos:altLabel>
