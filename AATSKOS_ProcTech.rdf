	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">liè huà</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">lie hua</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">lieh hua</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">barsten (procedé)</skos:prefLabel>
	<skos:prefLabel xml:lang="es">agrietado</skos:prefLabel>
