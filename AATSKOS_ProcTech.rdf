<rdf:Description rdf:about="http://vocab.getty.edu/aat/300192850">
	<skos:prefLabel xml:lang="en">fill light (lighting technique)</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">ophelderlicht</skos:prefLabel>
	<skos:prefLabel xml:lang="es">luz ambiental</skos:prefLabel>
	<skos:altLabel xml:lang="en">fill (lighting technique)</skos:altLabel>
