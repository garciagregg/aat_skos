<rdf:Description rdf:about="http://vocab.getty.edu/aat/300379768">
	<skos:prefLabel xml:lang="en">sorption</skos:prefLabel>
	<skos:altLabel xml:lang="en">sorbing</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The act of sorbing, the taking up and holding of one substance by another. Sorption encompasses the actions of absorption and adsorption performed individually or in combination.</skos:scopeNote>
