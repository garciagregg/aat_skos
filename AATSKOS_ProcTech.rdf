	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2017-08-25T10:46:32</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300240203">
