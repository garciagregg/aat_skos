	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-04-13T12:03:52</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300245805">
	<skos:prefLabel xml:lang="en">flat-woven</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">vlakweven</skos:prefLabel>
	<skos:prefLabel xml:lang="es">tejido liso</skos:prefLabel>
	<skos:altLabel xml:lang="nl">vlak weefsel</skos:altLabel>
	<skos:altLabel xml:lang="es">tejido</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Weaving process in which no pile, hand-knotting, or hand-tufting is used, typically with reference to rugs, towels, or other textiles that typically may be expected to have a pile.</skos:scopeNote>
	<skos:scopeNote xml:lang="nl">Een weefsel dat geen pool van uitstekende draden of lussen heeft.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300245451"/>
	<skos:hasTopConcept rdf:resource="http://vocab.getty.edu/aat/300264090"/>
	<dc:identifier>300245805</dc:identifier>
