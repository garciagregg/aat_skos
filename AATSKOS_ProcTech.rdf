<rdf:Description rdf:about="http://vocab.getty.edu/aat/300237903">
	<skos:prefLabel xml:lang="en">digital imaging</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">digitale beeldverwerking</skos:prefLabel>
	<skos:prefLabel xml:lang="es">proyección de imagen digital</skos:prefLabel>
