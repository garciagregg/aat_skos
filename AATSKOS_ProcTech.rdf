<rdf:Description rdf:about="http://vocab.getty.edu/aat/300391300">
	<skos:prefLabel xml:lang="en">x-ray absorption near-edge structure</skos:prefLabel>
	<skos:altLabel xml:lang="en">structure, x-ray absorption near-edge</skos:altLabel>
	<skos:altLabel xml:lang="en">XANES</skos:altLabel>
	<skos:altLabel xml:lang="en">near-edge X-ray absorption fine structure spectroscopy</skos:altLabel>
	<skos:altLabel xml:lang="en">NEXAFS</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">X-ray absorption spectroscopy technique that analyzes the modelling of the dominant edge feature, and its pre-edge, that occur at the frequencies that correspond to transitions between atomic levels.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300391299"/>
