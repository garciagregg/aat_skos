	<skos:prefLabel xml:lang="zh-hant">定向照明</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">dìng xiàng zhào míng</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">ding xiang zhao ming</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">ting hsiang chao ming</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">gerichte belichting</skos:prefLabel>
	<skos:prefLabel xml:lang="es">iluminación direccional</skos:prefLabel>
