	<skos:prefLabel xml:lang="es">iluminación de detalle</skos:prefLabel>
	<skos:altLabel xml:lang="en">accent light</skos:altLabel>
	<skos:altLabel xml:lang="en">light, accent</skos:altLabel>
	<skos:altLabel xml:lang="en">lighting, accent</skos:altLabel>
	<skos:altLabel xml:lang="es">iluminación semi drecta</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Localized lighting technique designed to emphasize a particular object or area.</skos:scopeNote>
	<skos:scopeNote xml:lang="nl">Techniek voor plaatselijke belichting om een bepaald object of gebied uit te lichten.</skos:scopeNote>
	<skos:scopeNote xml:lang="es">Técnica de iluminación localizada diseñada para enfatizar un determinado objeto o área.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300052002"/>
