	<skos:prefLabel xml:lang="nl">handzetsel</skos:prefLabel>
	<skos:prefLabel xml:lang="es">composición manual</skos:prefLabel>
	<skos:altLabel xml:lang="en">composition, hand</skos:altLabel>
	<skos:altLabel xml:lang="en">hand-setting</skos:altLabel>
	<skos:altLabel xml:lang="en">hand-set type</skos:altLabel>
	<skos:altLabel xml:lang="en">handset type</skos:altLabel>
