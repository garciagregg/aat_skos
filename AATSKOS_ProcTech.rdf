	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">zhong guo jiao zhi zhi tie fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">chung kuo chiao chih chih t'ieh fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">papier collé</skos:prefLabel>
	<skos:altLabel xml:lang="en">chine colle (proess)</skos:altLabel>
