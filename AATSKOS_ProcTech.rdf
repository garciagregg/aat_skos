<rdf:Description rdf:about="http://vocab.getty.edu/aat/300404910">
	<skos:prefLabel xml:lang="en">casein painting (technique)</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">caseïneschilderen (techniek)</skos:prefLabel>
