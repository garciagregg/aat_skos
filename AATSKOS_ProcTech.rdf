	<skos:prefLabel xml:lang="zh-hant">編號</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">biān hào</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">nummeren</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">bian hao</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">pien hao</skos:prefLabel>
