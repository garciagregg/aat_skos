<rdf:Description rdf:about="http://vocab.getty.edu/aat/300240211">
	<skos:prefLabel xml:lang="en">electrolytic process (copying)</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">elektrolytisch procedé (kopiëren)</skos:prefLabel>
