<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053544">
	<skos:prefLabel xml:lang="en">developing (photography)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">顯影 (攝影)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">xiǎn yǐng</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">xian ying</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">hsien ying</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">ontwikkelen</skos:prefLabel>
	<skos:prefLabel xml:lang="es">revelado (fotografía)</skos:prefLabel>
	<skos:altLabel xml:lang="en">developed (photography)</skos:altLabel>
	<skos:altLabel xml:lang="en">development (photography)</skos:altLabel>
