	<skos:altLabel xml:lang="en">electron microanalysis</skos:altLabel>
	<skos:altLabel xml:lang="en">electron probe micro-analysis</skos:altLabel>
	<skos:altLabel xml:lang="en">EPMA</skos:altLabel>
	<skos:altLabel xml:lang="en">microanalysis, electron probe</skos:altLabel>
	<skos:altLabel xml:lang="en">micro-analysis, electron probe</skos:altLabel>
	<skos:altLabel xml:lang="en">probe microanalysis, electron</skos:altLabel>
