	<skos:altLabel xml:lang="en">photography, advertising</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The activity of creating photographs to promote products or services to the public. This is often commissioned or produced by advertising agencies.</skos:scopeNote>
