<rdf:Description rdf:about="http://vocab.getty.edu/aat/300400885">
	<skos:prefLabel xml:lang="en">sponging (cleaning)</skos:prefLabel>
	<skos:altLabel xml:lang="en">sponged (cleaning)</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The action of washing or wiping with a sponge.</skos:scopeNote>
