<rdf:Description rdf:about="http://vocab.getty.edu/aat/300389873">
	<skos:prefLabel xml:lang="en">direct temperature-resolved mass spectrometry</skos:prefLabel>
	<skos:altLabel xml:lang="en">direct temperature mass spectrometry</skos:altLabel>
	<skos:altLabel xml:lang="en">spectrometry, direct temperature-resolved mass</skos:altLabel>
	<skos:altLabel xml:lang="en">DTMS (direct temperature-resolved mass spectrometry)</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A form of pyrolysis mass spectrometry in which the temperature of the sample is raised gradually to separate compounds in the evaporation phase from those in the crosslinked fraction of the pyrolysis phase. It is used to provide qualitative views of the broad-band molecular-level characteristics of complex organic samples.  </skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300389872"/>
	<skos:hasTopConcept rdf:resource="http://vocab.getty.edu/aat/300264090"/>
