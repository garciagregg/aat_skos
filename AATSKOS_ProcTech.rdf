	<skos:prefLabel xml:lang="es">talla (proceso)</skos:prefLabel>
	<skos:altLabel xml:lang="en">wood block printing (woodcut printing process)</skos:altLabel>
	<skos:altLabel xml:lang="en">wood-block printing (woodcut printing process)</skos:altLabel>
	<skos:altLabel xml:lang="en">woodcut printing</skos:altLabel>
