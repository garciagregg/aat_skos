<rdf:Description rdf:about="http://vocab.getty.edu/aat/300263603">
	<skos:prefLabel xml:lang="en">aldehyde tanning</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">aldehyde looien</skos:prefLabel>
	<skos:prefLabel xml:lang="es">curtido en aldehido</skos:prefLabel>
	<skos:altLabel xml:lang="en">aldehyde-tanned</skos:altLabel>
	<skos:altLabel xml:lang="en">aldehyde tannage</skos:altLabel>
