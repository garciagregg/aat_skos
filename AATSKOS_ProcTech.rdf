	<skos:prefLabel xml:lang="en">tacheometry</skos:prefLabel>
	<skos:altLabel xml:lang="en">tachymetry</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
