	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">jin duan zhi fa</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">chin tuan chih fa</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">brocheren</skos:prefLabel>
	<skos:prefLabel xml:lang="es">brocado</skos:prefLabel>
