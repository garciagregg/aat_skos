<rdf:Description rdf:about="http://vocab.getty.edu/aat/300254981">
	<skos:prefLabel xml:lang="en">blue stain</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">青變</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">qīng biàn</skos:prefLabel>
