<rdf:Description rdf:about="http://vocab.getty.edu/aat/300390518">
	<skos:prefLabel xml:lang="en">matrix-assisted laser desorption ionization</skos:prefLabel>
	<skos:altLabel xml:lang="en">MALDI</skos:altLabel>
	<skos:altLabel xml:lang="en">matrix-assisted laser desorption/ionization</skos:altLabel>
	<skos:altLabel xml:lang="en">ionization, matrix-assisted laser desorption</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Method of ionizing samples for use in mass spectrometry allowing the analysis of fragile biomolecules and organic molecules which fragment when ionized by other methods. The analyte is prepared in or on an organic matrix that is coated on a surface which is then irradiated by laser photons, removing the analyte from the surface and ionizing it for analysis.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300220362"/>
