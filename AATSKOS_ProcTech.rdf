	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-22T08:43:51</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300054718">
