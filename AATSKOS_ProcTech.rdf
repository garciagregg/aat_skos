	<skos:prefLabel xml:lang="nl">aanrijgen</skos:prefLabel>
	<skos:prefLabel xml:lang="es">encordonar</skos:prefLabel>
	<skos:altLabel xml:lang="en">laced-in</skos:altLabel>
