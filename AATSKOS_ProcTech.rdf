<rdf:Description rdf:about="http://vocab.getty.edu/aat/300379952">
	<skos:prefLabel xml:lang="en">opacification</skos:prefLabel>
	<skos:altLabel xml:lang="en">opacified</skos:altLabel>
	<skos:altLabel xml:lang="en">opacifying</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
