	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">mei shi xue</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">mei shih hsüeh</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">gastronomie</skos:prefLabel>
	<skos:prefLabel xml:lang="fr">gastronomie</skos:prefLabel>
	<skos:prefLabel xml:lang="es">gastronomía</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
