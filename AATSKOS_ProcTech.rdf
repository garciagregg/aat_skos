	<skos:prefLabel xml:lang="en">dip brazing</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">熱浸硬焊</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">rè jìn yìng hàn</skos:prefLabel>
