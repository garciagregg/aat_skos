	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-01-07T12:56:27</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053249">
	<skos:prefLabel xml:lang="en-us">photoaquatint (process)</skos:prefLabel>
	<skos:prefLabel xml:lang="es">fotoaguatinta</skos:prefLabel>
	<skos:prefLabel xml:lang="en-gb">photo-aquatint (process)</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">heliogravure (procedé)</skos:prefLabel>
	<skos:altLabel xml:lang="en-us">aquatint (photomechanical process)</skos:altLabel>
	<skos:altLabel xml:lang="en-us">aquatint engraving</skos:altLabel>
