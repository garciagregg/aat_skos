	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">gao su she ying shu</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">kao su she ying shu</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">hogesnelheidsfotografie</skos:prefLabel>
	<skos:prefLabel xml:lang="es">fotografía ultrarrápida (fotografía del momento)</skos:prefLabel>
