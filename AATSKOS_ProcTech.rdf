<rdf:Description rdf:about="http://vocab.getty.edu/aat/300051994">
	<skos:prefLabel xml:lang="en">general diffuse lighting</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">全面漫射照明</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">quán miàn màn shè zhào míng</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">quan mian man she zhao ming</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">ch'üan mien man she chao ming</skos:prefLabel>
