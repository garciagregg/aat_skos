	<skos:prefLabel xml:lang="zh-latn-wadegile">ch'ing pien</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">blauwkleuring</skos:prefLabel>
	<skos:prefLabel xml:lang="es">azuleado</skos:prefLabel>
	<skos:altLabel xml:lang="en">blue rot</skos:altLabel>
