<rdf:Description rdf:about="http://vocab.getty.edu/aat/300380031">
	<skos:prefLabel xml:lang="en">stereoscopy</skos:prefLabel>
	<skos:altLabel xml:lang="en">stereoscopic</skos:altLabel>
