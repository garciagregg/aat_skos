	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-01-16T10:12:31</dct:modified>
</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053611">
