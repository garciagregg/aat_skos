</rdf:Description>
<rdf:Description rdf:about="http://vocab.getty.edu/aat/300053885">
	<skos:prefLabel xml:lang="en">heating (physicochemical process)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-hant">加熱 (物理化學的過程)</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-hanyu">jiā rè</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-pinyin-x-notone">jia re</skos:prefLabel>
	<skos:prefLabel xml:lang="zh-latn-wadegile">chia je</skos:prefLabel>
	<skos:prefLabel xml:lang="nl">verwarmen</skos:prefLabel>
	<skos:prefLabel xml:lang="es">calentamiento (proceso físico-quimico)</skos:prefLabel>
	<skos:altLabel xml:lang="nl">warmen</skos:altLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Process of adding heat in order to raise the temperature.</skos:scopeNote>
	<skos:broader rdf:resource="http://vocab.getty.edu/aat/300229488"/>
